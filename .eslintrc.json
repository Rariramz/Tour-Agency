{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "tsconfig.json",
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
    },
    "globals": {
        "__IS_DEV__": true,
    },
    "ignorePatterns": ["dist/**/*", "node_modules/**/*", "webpack/**/*", "webpack.config.ts"],
    "root": true,
    "settings": {
        "react": {
          "version": "detect",
        },
    },
}
